# CoinMarketCap API
# Get your API key from: https://coinmarketcap.com/api/
CMC_API_KEY=your_coinmarketcap_api_key_here

# Database Configuration
DB_HOST=localhost
DB_NAME=folyo
DB_USER=folyo_user
DB_PASS=your_secure_password_here
DB_CHARSET=utf8mb4

# Cache Configuration
# Driver: 'redis' or 'file' (file-based is fallback)
CACHE_DRIVER=redis
CACHE_PREFIX=folyo

# Redis Configuration (only used if CACHE_DRIVER=redis)
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here
REDIS_DATABASE=0
REDIS_TIMEOUT=2.0
